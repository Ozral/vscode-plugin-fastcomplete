{
  "name": "fastcomplete4j",
  "displayName": "FastComplete4J",
  "description": "Java Postfix",
  "publisher": "Howie Young",
  "version": "0.0.1",
  "keywords": [
    "postfix",
    "java",
    "complete"
  ],
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:fastcomplete4j.helloWorld",
    "onLanguage:java"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fastcomplete4j.helloWorld",
        "title": "Hello World"
      }
    ],
    "keybindings": [
      {
        "command": "fastcomplete4j.helloWorld",
        "key": "ctrl+f10",
        "mac": "cmd+f10",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  }
}
